<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>

  <display-name>My Project Management</display-name>
  <description>
    My Project Management
  </description>


  <servlet>
    <servlet-name>Login</servlet-name>
    <servlet-class>CourseSignIn</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>add</servlet-name>
    <servlet-class>DBSQLaddNetwork</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>query</servlet-name>
    <servlet-class>DBSQLqueryNetwork</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>Match</servlet-name>
    <servlet-class>Match</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>test</servlet-name>
    <servlet-class>DBSQLqueryNetwork</servlet-class>
  </servlet>	
  <servlet>
    <servlet-name>test2</servlet-name>
    <jsp-file>/ajax2mysql.jsp</jsp-file>
  </servlet>

  

 
  <!-- The mapping for the webdav servlet -->
  <servlet-mapping>
    <servlet-name>Match</servlet-name>
    <url-pattern>/Match</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Login</servlet-name>
    <url-pattern>/Login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>add</servlet-name>
    <url-pattern>/add</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>query</servlet-name>
    <url-pattern>/query</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>test</servlet-name>
    <url-pattern>/test</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>test2</servlet-name>
    <url-pattern>/test2</url-pattern>
  </servlet-mapping>
 

</web-app>
